{
    "manifest_version": 3,
    "name": "Daily Quote",
    "description": "Daily quote for Hinh",
    "version": "1.0",
    "permissions": ["storage"],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  